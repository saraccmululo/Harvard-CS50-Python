items = {}


def main():
    results = get_items()
    for result in sorted(results):
        print((results[result]), result.upper())


def get_items():
    while True:
        try:
            item = input()
            if item in items:
                items[item] += 1
            else:
                items[item] = 1

        except EOFError:
            return items


main()